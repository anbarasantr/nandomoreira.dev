@import '~stylus/index.styl'

.list
  padding 0
  display flex
  align-items center
  span, a
    display inline-block
    margin-right .625rem
    svg
      padding .25rem
      opacity 0.75

.tag
  padding .1875rem .75rem
  font-size .75rem
  background-color #fff
  border 1px solid rgba(#000, .1)
  border-radius 3.75rem
